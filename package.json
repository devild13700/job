    "dayjs": "^1.11.12",
    "dotenv": "^16.4.1",
    "lucide-react": "^0.368.0",
    "mongoose": "^8.3.2",
    "next": "14.2.1",
    "next-themes": "^0.3.0",
    "query-string": "^9.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-type-animation": "^3.2.0",
    "stripe": "^15.5.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "vaul": "^0.9.0"
